    "bcryptjs": "^2.4.3",
    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.13.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "react-router-dom": "^5.2.0",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "@types/react-router-dom": "^5.3.2",
    "concurrently": "^6.3.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.14"
  }
}
